#!/Users/magyeong-ug/workspace/MyChainForKISA/venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'mychain','console_scripts','mychain'
__requires__ = 'mychain'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mychain', 'console_scripts', 'mychain')()
    )
